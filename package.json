{
  "name": "convergence",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "clean": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && rm -rf node_modules && rm -rf .pnpm-store && rm -rf  pnpm-lock.yaml && pnpm --ignore-workspace install"
  },
  "dependencies": {
    "@googlemaps/markerclusterer": "^2.5.3",
    "@libsql/client": "^0.14.0",
    "@payloadcms/db-sqlite": "3.9.0",
    "@payloadcms/live-preview-react": "3.9.0",
    "@payloadcms/next": "3.9.0",
    "@payloadcms/payload-cloud": "3.9.0",
    "@payloadcms/plugin-form-builder": "3.9.0",
    "@payloadcms/plugin-nested-docs": "3.9.0",
    "@payloadcms/plugin-redirects": "3.9.0",
    "@payloadcms/plugin-search": "3.9.0",
    "@payloadcms/plugin-seo": "3.9.0",
    "@payloadcms/richtext-lexical": "3.9.0",
    "@payloadcms/ui": "3.9.0",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-slot": "^1.1.1",
    "@vis.gl/react-google-maps": "^1.4.2",
    "babel-plugin-react-compiler": "19.0.0-beta-55955c9-20241229",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "geist": "^1.3.1",
    "graphql": "^16.10.0",
    "libsql": "0.5.0-pre.5",
    "lucide-react": "^0.378.0",
    "next": "^15.1.3",
    "next-sitemap": "^4.2.3",
    "payload": "3.9.0",
    "payload-admin-bar": "^1.0.6",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "7.45.4",
    "sharp": "0.32.6",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@tailwindcss/typography": "^0.5.15",
    "@trivago/prettier-plugin-sort-imports": "^5.2.1",
    "@types/escape-html": "^1.0.4",
    "@types/node": "22.5.4",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.1",
    "autoprefixer": "^10.4.20",
    "copyfiles": "^2.4.1",
    "eslint": "^9.17.0",
    "eslint-config-next": "15.1.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tailwindcss": "^3.4.17",
    "typescript": "5.7.2"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  }
}
